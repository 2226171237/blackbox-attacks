set terminal epslatex size 6cm, 12cm

set output 'latex_plots/query_iter_hist.tex'

set key at graph 0.5, 1.1 horizontal samplen 0.4
set style data histogram
set style histogram cluster gap 1
set style fill solid border -1
set boxwidth 0.8
set xtic rotate by 90 scale 0 right
unset ytics

set ylabel '\small{\textgm{IFD-QR (PCA-400, logit)} across models}'
set y2tics rotate by 90 center
set yrange [0:100]; set xrange [-0.5:3.5]
set y2label '\small{Adversarial success (\%)}' offset -2
set xlabel ' '
set tmargin 6
set bmargin 6
set label 2 'Untargeted' at graph 0.15, 1.11 left rotate by 90
set label 3 'Targeted' at graph 0.41, 1.11 left rotate by 90

plot 'iterative_qr_fd.txt' using 2:xtic(1) ti col, '' using 3 ti col
