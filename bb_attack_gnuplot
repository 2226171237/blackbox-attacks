'output_data/baseline/baseline_linf_md_rand_0.51_modelA_adv.txt' u 2:3 with lp ls 2 title '\small{Rand.}', \
'output_data/untargeted/query_based_un_xent_linf_modelA_adv.txt' u 2:3 with lp ls 3 title '\small{FD (xent)}', \
'output_data/untargeted/query_based_un_cw_linf_modelA_adv.txt' u 2:3 with lp ls 4 title '\small{FD (logit)}', \
'output_data/blind_transfer/modelB_to_modelA_adv.txt' u 1:2 with lp ls 5 title '\small{B (FGS, xent)}',\
'output_data/blind_transfer/modelB_to_modelA_adv_cw.txt' u 1:2 with lp ls 6 title '\small{B (FGS, logit)}',\
'output_data/untargeted/wb_un_cw_linf_modelA_adv.txt' u 1:2 with lp ls 10 title '\small{WB FGS (logit)}',\
'output_data/untargeted/wb_un_xent_linf_modelA_adv.txt' u 1:2 with lp ls 26 title '\small{WB FGS (xent)}',\
'output_data/untargeted/query_based_un_cw_linf_modelA_adv0.05_rand.txt' u 2:3 with lp ls 31 title '\small{Rand FD}', \
'output_data/untargeted/wb_un_cw_linf_modelA_adv0.05_rand.txt' u 1:2 with lp ls 7 title '\small{Rand FGS}'

unset output

