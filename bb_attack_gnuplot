set output "latex_plots/finite_diff_baseline_linf_Model_A.tex"
# set terminal pngcairo dashed size 16cm, 12cm
# set output "latex_plots/finite_diff_baseline_linf_Model_A.png"

set xlabel '$\epsilon$' offset 0, 0.5
set ylabel 'Adversarial success (\%)' offset 2
set xrange [0:0.4]
set yrange [0:100]
# set margins 3, 1, 2, 2
set key out vert
set key right
set key width -10
set key samplen 1 spacing 1
set title '$L_{\infty}$ constrained strategies on Model A'

plot 'output_data/baseline/baseline_linf_md_modelA.txt' u 1:2 with lp ls 1 title '\small{\dom}', \
'output_data/baseline/baseline_linf_md_rand_0.51_modelA.txt' u 2:3 with lp ls 2 title '\small{\rand}', \
'output_data/untargeted/query_based_un_xent_linf_modelA.txt' u 2:3 with lp ls 3 title '\small{FD (xent)}', \
'output_data/untargeted/query_based_un_cw_linf_modelA.txt' u 2:3 with lp ls 4 title '\small{FD (logit)}', \
'output_data/untargeted/query_based_un_cw_linf_modelApca_100.txt' u 2:3 with lp ls 5 title '\small{ QR (PCA-100, logit)}', \
'output_data/blind_transfer/modelB_to_modelA.txt' u 1:2 with lp ls 6 title '\small{B (FGS, xent)}',\
'output_data/blind_transfer/modelB_to_modelA_cw.txt' u 1:2 with lp ls 10 title '\small{B (FGS, logit)}',\
'output_data/untargeted/wb_un_cw_linf_modelA.txt' u 1:2 with lp ls 26 title '\small{WB FGS (logit)}',\
'output_data/untargeted/wb_un_xent_linf_modelA.txt' u 1:2 with lp ls 33 title '\small{WB FGS (xent)}'

unset output

set output "latex_plots/finite_diff_baseline_linf_Model_A_adv.tex"

